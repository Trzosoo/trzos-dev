{
  "name": "trzos-dev",
  "description": "Bartosz Trzos personal page",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "npm run build:tailwind && sapper build --legacy",
    "export": "sapper export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "watch:tailwind": "postcss static/css/tailwind.css -o static/css/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/css/tailwind.css -o static/css/index.css"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "autoprefixer": "^9.7.3",
    "postcss-import": "^12.0.1",
    "prettier": "1.19.1",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.0",
    "svelte": "^3.49.0",
    "tailwindcss": "^1.1.4"
  }
}
